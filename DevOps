git config --list

remove: git config --unset --user.name
        git config --unset --user.email
  git config --list
-------------------------------------------------------
 
~ ls
~ ls -a
~touch file1
~ls
file1
git status(untracked file: red clr file1== it is in workspace phase)
~git add file1
git status(tracked file: green clr file1== it is in staging/index phase)
~git commit -m "label" file1
               ("commit data1")
git status(dont show anything== it is in local repository)
git log
git show cid(select the above logid and( to enter within git show press"shift-insert" and come to the command))
